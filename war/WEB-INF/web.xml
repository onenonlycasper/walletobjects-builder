<?xml version="1.0" encoding="utf-8" standalone="no"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>ClassList</servlet-name>
    <servlet-class>com.google.java.wob.ClassListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClassList</servlet-name>
    <url-pattern>/classlist</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ObjList</servlet-name>
    <servlet-class>com.google.java.wob.ObjectListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ObjList</servlet-name>
    <url-pattern>/objlist</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Save</servlet-name>
    <servlet-class>com.google.java.wob.SaveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Save</servlet-name>
    <url-pattern>/save</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Patch</servlet-name>
    <servlet-class>com.google.java.wob.PatchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Patch</servlet-name>
    <url-pattern>/patch</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Search</servlet-name>
    <servlet-class>com.google.java.wob.SearchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Search</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JWT</servlet-name>
    <servlet-class>com.google.java.wob.SaveToWalletServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JWT</servlet-name>
    <url-pattern>/jwt</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Add Message</servlet-name>
    <servlet-class>com.google.java.wob.AddMessageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Add Message</servlet-name>
    <url-pattern>/addmessage</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
